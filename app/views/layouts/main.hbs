  <!-- Google tag (gtag.js) -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=G-MPCCBMHYPB"></script>
  <script>
    if (window.location.hostname === 'wilsonespina.com') {
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'G-MPCCBMHYPB');
    }
  </script>

  <body>
    <main id="resume" class="page right-border">
      {{> resume-header}}
      <div class="resume-content">
          {{> about }}
          {{> summary }}
          {{> skills }}
          {{> work }}
          {{> volunteer }}
          {{> education }}
          {{> awards }}
          {{> publications }}
          {{> references }}
          {{> languages }}
          {{> interests }}
          <div class="break"></div>
        </div>

