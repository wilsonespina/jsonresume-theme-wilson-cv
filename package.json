    "gulp-serve": "^1.4.0",
    "gulp-size": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-useref": "^4.0.0",
    "gulp-wrap": "^0.15.0",
    "lazypipe": "^1.0.1"
  },
  "dependencies": {
    "address-format": "0.0.3",
    "handlebars": "^4.7.7",
    "handlebars-wax": "^6.1.0",
    "moment": "^2.29.4",
    "node-sass-middleware": "^1.0.1",
    "resume-cli": "3.0.8",
    "swag": "^0.7.0"
  }
