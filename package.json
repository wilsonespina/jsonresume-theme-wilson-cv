{
  "author": "Kelvin Nguyen",
  "name": "jsonresume-theme-caffeine",
  "description": "Caffeine theme for JSON Resume",
  "version": "2.0.1",
  "keywords": [
    "json",
    "resume",
    "jsonresume",
    "json-resume"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kelyvin/jsonresume-theme-caffeine"
  },
  "license": "MIT",
  "scripts": {
    "start": "npx gulp && npx resume serve --theme .",
    "dev": "nodemon --ext js,hbs,scss,css,json",
    "watch": "nodemon -e scss -x 'npm run css'",
    "css": "node-sass --include-path scss ./app/styles/main.scss ./style.css",
    "export": "npx gulp && npx resume export resume --format pdf --theme ."
  },
  "devDependencies": {
    "browser-sync": "^2.27.10",
    "connect-livereload": "^0.6.1",
    "del": "^5.1.0",
    "express": "^4.17.3",
    "express-handlebars": "^6.0.3",
    "handlebars": "^4.7.7",
    "livereload": "^0.9.3",
    "node-sass": "^7.0.3",
    "nodemon": "^2.0.19",
    "opn": "^6.0.0",
    "require-dir": "^1.2.0",
    "sass": "^1.49.9",
    "serve-index": "^1.8.0",
    "yargs": "^15.0.2"
  },
  "dependencies": {
    "address-format": "0.0.3",
    "handlebars": "^4.0.5",
    "handlebars-wax": "^6.1.0",
    "moment": "^2.29.4",
    "node-sass-middleware": "^1.0.1",
    "resume-cli": "2.2.4",
    "swag": "^0.7.0"
  }
}
