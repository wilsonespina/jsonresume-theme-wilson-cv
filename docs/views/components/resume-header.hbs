{{#resume.basics}}
<header class="resume-header {{#if image}}minus-margin{{/if}}">
  <div class="profile-header pull-left">
    <h1>{{name}}</h1>
    <h2>{{label}}</h2>
  </div>
  <div class="profile-pic pull-right">
    {{#if image}}
      <img src="{{image}}" alt="profile-pic">
    {{/if}}
  </div>
</header>
{{/resume.basics}}
